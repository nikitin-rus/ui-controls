{
    "compilerOptions": {
        "strict": true,
        "module": "ES6",
        "target": "ES6",
        "moduleResolution": "Bundler",
        "sourceMap": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "skipLibCheck": true,
    },
    "include": [
        "src/**/*",
    ],
    "exclude": [
        "src/**/*.stories.tsx",
    ]
}